
{
	"configure" : 
	{
		"" : [ "--enable-always", "--with-curses=$(hl_prefix curses)" ],
		"+profile" : [ "--enable-profile" ],
		"+ssl & +openssl" : [ "--enable-profile" ],
		"-profile" : [ "--disable-profile" ],
		"debug" : [ "--$enable-debug" ]
	},
	"conflicts" : 
	{
		"install" : [ "run make install" ],
		"prepare" : [ "./autogen.sh" ]
	},
	"deps" : 
	[
		"make",
		"gcc > 3.4 < 4.8 ! 4.5.8",
		"install = 4.5",
		"curses? curses:4[+shell,-gui,lst,#requireIfParentHas] >3.4 <4.8 <=4.8 !4.5.8 ~4\\.5\\.[0-9]+"
	],
	"flags" : 
	{
		"C" : [ "-O3", "!-ffastmath" ],
		"CXX" : [ "--stdc++11" ],
		"F" : [ "" ],
		"LD" : [ "-L/usr/lib" ]
	},
	"homepage" : "",
	"host" : 
	{
		"debian8" : [ "bash" ],
		"default" : true
	},
	"inherit" : "models/autotools",
	"md5" : null,
	"module" : "gcc-${VERSION}",
	"name" : "app-shell/bash",
	"scripts" : [ "models/scripts/default.sh" ],
	"steps" : 
	{
		"install" : [ "run make install" ],
		"prepare" : [ "./autogen.sh" ]
	},
	"subdir" : "bash-${VERSION}",
	"use" : [ "-debug", "-expat" ],
	"vars" : 
	{
		"PYTHON_SLOT" : "3"
	},
	"versions" : [ "4.0", "4.1", "4.2", "4.3" ],
	"vfetcher" : 
	{
		"mode" : "",
		"regexp" : "",
		"url" : ""
	},
	"vspecific" : 
	{
		"4.2" : 
		{
			"configure" : 
			{
				"" : [ "--enable-bugfix" ]
			}
		},
		":4 >3.4 <4.8 !4.5.8 ~4\\.5\\.[0-9]+" : 
		{
			"configure" : 
			{
				"" : [ "!--enable-bugfix" ]
			}
		}
	},
	"warn" : [ "Some warning to print (NOT YET SUPPORTED)" ]
}
