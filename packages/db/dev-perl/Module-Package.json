{
	"name": "dev-perl/Module-Package",
	"homepage": "http://search.cpan.org/~ingy/Module-Package-0.30/",
	"versions": [
		"0.30"
	],
	"inherit": "models/perl",
	"vfetcher": {
		"mode": "http",
		"url": "http://search.cpan.org/~ingy/",
		"regexp": "Module-Package-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*)"
	},
	"md5": {},
	"subdir": "Module-Package-${VERSION}",
	"urls": [
		"http://search.cpan.org/CPAN/authors/id/I/IN/INGY/Module-Package-${VERSION}.tar.gz"
	],
	"deps": [
	],
	"configure": {},
	"use": [
		""
	],
	"steps": {
		"prepare": [
			"run_sh export PERL5LIB=$PWD:$PERL5LIB"
		]
	},
	"host": {
		"default": false,
		"debian8": [
			"Module-Package"
		],
		"centos7": [
			"Module-Package"
		],
		"gentoo": [
			"dev-perl/Module-Package"
		]
	}
}
