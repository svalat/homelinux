{
	"name": "app-arch/cpio",
	"homepage": "https://www.gnu.org/software/cpio/cpio.html",
	"inherit": "models/autotools",
	"versions": [ 
		"2.12"
	],
	"vfetcher": {
		"mode": "http",
		"url": "http://ftp.gnu.org/gnu/cpio/",
		"regexp": "^cpio-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).tar.gz"
	},
	"md5" : {},
	"subdir": "cpio-${VERSION}",
	"urls" : [
		"http://ftp.gnu.org/gnu/cpio/cpio-${VERSION}.tar.gz"
	],
	"deps": [
	],
	"configure": {
		"nls": [ "--$enable-nls" ]
	},
	"host": {
		"default": false,
		"debian8": [ "cpio" ],
		"centos7": [ "cpio" ]
	},
	"use": [ 
		"nls"
	]
}
