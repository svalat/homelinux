{
	"name": "dev-qt/qt-meta",
	"homepage": "https://www.qt.io/",
	"inherit": "models/virtual",
	"versions": [ 
		"4.8.6"
	],
	"vfetcher": {
		"mode": "http",
		"url": "http://download.qt.io/archive/qt/4.8/",
		"regexp": "^([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*)/"
	},
	"md5" : {},
	"subdir": "qt-everywhere-opensource-src-${VERSION}",
	"urls" : [
		"http://download.qt.io/archive/qt/$(echo $VERSION | cut -d '.' -f 1-2)/${VERSION}/qt-everywhere-opensource-src-${VERSION}.tar.gz"
	],
	"deps": [
		"dev-qt/assistant :4",
    	"dev-qt/designer :4",
    	"dev-qt/linguist :4",
    	"dev-qt/pixeltool :4",
    	"dbus? dev-qt/qdbusviewe : 4",
    	"qt3support? dev-qt/qt3support :4",
    	"dev-qt/qtbearer :4",
    	"dev-qt/qtcore :4",
    	"dbus? dev-qt/qtdbus :4",
    	"dev-qt/qtdeclarative :4",
    	"examples? dev-qt/qtdemo :4",
    	"dev-qt/qtgui :4",
    	"dev-qt/qthelp :4",
    	"dev-qt/qtmultimedia :4",
    	"dev-qt/qtopengl :4",
    	"openvg? dev-qt/qtopenvg :4",
    	"kde? media-libs/phonon[qt4]",
    	"-kde? dev-qt/qtphonon :4",
    	"dev-qt/qtscript :4",
    	"dev-qt/qtsql :4",
    	"dev-qt/qtsvg :4",
    	"dev-qt/qttest :4",
    	"webkit? dev-qt/qtwebkit :4",
    	"dev-qt/qtxmlpatterns :4"
	],
	"configure": {
	},
	"host": {
		"default": false,
		"debian8": [ "qt4" ]
	},
	"use": [ 
		"+debus",
		"examples",
		"kde",
		"openvg",
		"+qt3support",
		"+webkit"
	],
	"slots" : {
	    "4": ">=4.0 <5.0"
	    "5": ">5.0"
	}
}
