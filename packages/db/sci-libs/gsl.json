{
	"name": "sci-libs/gsl",
	"homepage": "https://www.gnu.org/software/gsl/",
	"inherit": "models/autotools",
	"versions": [ 
		"1.16" 
	],
	"vfetcher": {
		"mode": "ftp",
		"url": "ftp://ftp.gnu.org/gnu/gsl/",
		"regexp": "gsl-([0-9]+.[0-9]+.?[0-9]*.?[0-9]*).(tar.gz|tar.bz2|tar.xz)"
	},
	"md5" : {},
	"subdir": "gsl-${VERSION}",
	"urls" : [
		"http://ftpmirror.gnu.org/gnu/gsl/gsl-${VERSION}.tar.gz",
		"ftp://ftp.gnu.org/gnu/gsl/gsl-${VERSION}.tar.gz"
	],
	"deps": [
	],
	"configure": {
	},
	"host": {
		"default": false,
		"debian8": [ "libgsl0-dev" ],
		"centos7": [ "gsl", "gsl-devel" ],
		"gentoo": [ "sci-libs/gsl" ]
	}
}
