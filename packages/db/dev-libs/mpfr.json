{
	"name": "dev-libs/mpfr",
	"homepage": "http://www.mpfr.org/",
	"inherit": "models/autotools",
	"versions": [ 
		"3.1.3" 
	],
	"vfetcher": {
		"mode": "ftp",
		"url": "ftp://ftp.gnu.org/gnu/mpfr/",
		"regexp": "mpfr-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).tar.bz2"
	},
	"md5" : {},
	"subdir": "mpfr-${VERSION}",
	"urls" : [
		"ftp://ftp.gnu.org/gnu/mpfr/mpfr-${VERSION}.tar.bz2"
	],
	"deps": [
		"dev-libs/gmp"
	],
	"configure": {
		"": [ "$(hl_with gmp dev-libs/gmp)" ]
	},
	"host": {
		"default": false,
		"debian8": [ "libmpfr-dev" ],
		"centos7": [ "libmpfr", "libmpfr-devel" ],
		"gentoo": [ "dev-libs/mpfr" ]
	}
}
