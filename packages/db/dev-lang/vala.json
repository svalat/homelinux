{
	"api": 1,
	"name": "dev-lang/vala",
	"homepage": "https://wiki.gnome.org/Projects/Vala",
	"versions": [
		"0.34.8"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "http-gnome-cache",
		"url": "http://ftp.gnome.org/pub/GNOME/sources/vala/cache.json",
		"regexp": "vala-common-([0-9]+.[0-9]+.?[0-9]*.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz|zip|tbz2)"
	},
	"md5": {},
	"subdir": "vala-${VERSION}",
	"urls": [
		"http://ftp.gnome.org/pub/GNOME/sources/vala/${SVERSION}/vala-${VERSION}.tar.xz"
	],
	"deps": [
	],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"vala","vala-dev"
		],
		"centos7": [
			"vala", "vala-devel"
		],
		"gentoo": [
			"dev-lang/vala", "dev-libs/valat-common"
		]
	}
}
