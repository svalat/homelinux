{
	"name": "dev-cpp/cairomm",
	"homepage": "http://cairographics.org/cairomm",
	"inherit": "models/autotools",
	"versions": [ 
		"1.12.0" 
	],
	"vfetcher": {
		"mode": "http",
		"url": "http://cairographics.org/releases/",
		"regexp": "^cairomm-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).tar.gz"
	},
	"md5" : {},
	"subdir": "cairomm-${VERSION}",
