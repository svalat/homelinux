{
	"api": 1,
	"name": "app-crypt/qca",
	"homepage": "https://userbase.kde.org/QCA",
	"versions": [
		"2.1.3"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "html",
		"url": "https://userbase.kde.org/QCA",
		"regexp": "qca-([0-9]+.[0-9]+.?[0-9]*.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz|zip|tbz2)"
	},
	"md5": {},
	"subdir": "qca-${VERSION}",
	"urls": [
		"http://download.kde.org/stable/qca/${VERSION}/src/qca-${VERSION}.tar.xz"
	],
	"deps": [
		"!app-crypt/qca-cyrus-sasl",
		"!app-crypt/qca-gnupg",
		"!app-crypt/qca-logger",
		"!app-crypt/qca-ossl",
		"!app-crypt/qca-pkcs11",
		"botan? dev-libs/botan:0",
		"gcrypt? dev-libs/libgcrypt",
		"gpg? app-crypt/gnupg",
		"nss? dev-libs/nss",
		"pkcs11&-libressl? dev-libs/openssl:0",
		"pkcs11? dev-libs/pkcs11-helper",
		"pkcs11&libressl? dev-libs/libressl",
		"qt4? dev-qt/qt:4",
		"qt5? dev-qt/qt:5",
		"sasl? dev-libs/cyrus-sasl:2",
		"ssl&libressl? dev-libs/libressl",
		"doc? app-doc/doxygen"
	],
	"configure": {},
	"use": [
		"botan",
		"debug",
		"doc",
		"examples",
		"gcrypt",
		"gpg",
		"libressl",
		"logger",
		"nss",
		"pkcs11",
		"+qt4",
		"qt5",
		"sasl",
		"softstore",
		"+ssl",
		"test"
	],
	"host": {
		"default": false,
		"debian8": [
			"qca"
		],
		"centos7": [
			"qca"
		],
		"gentoo": [
			"app-crypt/qca"
		]
	}
}
