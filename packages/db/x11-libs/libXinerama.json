	"versions": [
		"1.1.3"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "http",
		"url": "http://xorg.freedesktop.org/releases/individual/lib/",
		"regexp": "libXinerama-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
