{
	"name": "dev-util/valgrind",
	"homepage": "http://www.valgrind.org",
	"inherit": "models/autotools",
	"versions": [ 
		"3.11.0"
	],
	"vfetcher": {
		"mode": "ftp",
		"url": "ftp://sourceware.org/pub/valgrind",
		"regexp": "valgrind-([0-9]+.[0-9]+.?[0-9]*.?[0-9]*).(tar.bz2|tar.xz)"
	},
	"md5" : {},
	"subdir": "valgrind-${VERSION}",
	"urls" : [
		"ftp://sourceware.org/pub/valgrind/valgrind-${VERSION}.tar.bz2"
	],
	"deps": [
	],
	"host": {
		"default": false,
		"debian8": [ "valgrind" ],
		"centos7": [ "valgrind" ],
		"gentoo": [ "dev-util/valgrind" ]
	}
}
