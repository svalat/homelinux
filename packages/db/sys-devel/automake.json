{
	"name": "sys-devel/automake",
	"homepage": "https://www.gnu.org/software/automake/",
	"inherit": "models/autotools",
	"versions": [ 
		"1.12.2"
	],
	"vfetcher": {
		"mode": "http",
		"url": "mirror://http-gnu/gnu/automake/",
		"regexp": "automake-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).tar.gz"
	},
	"md5" : {},
