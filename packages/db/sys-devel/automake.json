	"urls" : [
		"mirror://http-gnu/gnu/automake/automake-${VERSION}.tar.gz"
	],
	"deps": [
		"sys-devel/autoconf",
		"sys-apps/help2man"
	],
	"host": {
		"default": true,
		"debian8": [ "automake" ],
		"centos7": [ "automake" ],
		"gentoo": [ "sys-devel/automake" ]
	}
}
