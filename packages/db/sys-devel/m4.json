{
	"name": "sys-devel/m4",
	"homepage": "https://www.gnu.org/software/m4/m4.html",
	"versions": [
		"1.4.17"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "http",
		"url": "http://ftp.gnu.org/gnu/m4/",
		"regexp": "m4-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "m4-${VERSION}",
	"urls": [
		"http://ftp.gnu.org/gnu/m4/m4-${VERSION}.tar.bz2"
	],
	"deps": [
		"app-arch/xz-utils"
	],
	"configure": {
		"": [
			"--enable-changeword"
		]
	},
	"use": [
		"examples"
	],
	"host": {
		"default": false,
		"debian8": [
			"m4"
		]
	}
}
