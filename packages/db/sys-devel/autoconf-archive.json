{
	"api": 1,
	"name": "sys-devel/autoconf-archive",
	"homepage": "https://www.gnu.org/software/autoconf-archive/",
	"versions": [
		"2017.03.21"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "ftp",
		"url": "ftp://ftp.gnu.org/gnu/autoconf-archive/",
		"regexp": "autoconf-archive-([0-9]+.[0-9]+.?[0-9]*.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "autoconf-archive-${VERSION}",
	"urls": [
		"http://ftpmirror.gnu.org/autoconf-archive/autoconf-archive-${VERSION}.tar.xz"
	],
	"deps": [
		"app-arch/xz-utils"
	],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"autoconf-archive"
		],
		"centos7": [
			"autoconf"
		],
		"gentoo": [
			"sys-devel/autoconf-archive"
		]
	}
}
