{
	"name": "app-vim/nerdtree-tabs",
	"homepage": "https://github.com/jistr/vim-nerdtree-tabs",
	"versions": [
		"1.4.7"
	],
	"inherit": "models/vim",
	"vfetcher": {
		"mode": "gibhub",
		"url": "github://jistr/vim-nerdtree-tabs",
		"regexp": "nerdtree-tabs-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "vim-nerdtree-tabs-${VERSION}",
	"urls": [
		"github://jistr/vim-nerdtree-tabs"
	],
	"deps": [
		"app-vim/nerdtree"
	],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"nerdtree-tabs"
		],
		"centos7": [
			"nerdtree-tabs"
		],
		"gentoo": [
			"app-vim/nerdtree-tabs"
		]
	}
}
