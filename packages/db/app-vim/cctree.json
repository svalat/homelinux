{
	"name": "app-vim/cctree",
	"homepage": "https://sites.google.com/site/vimcctree/",
	"versions": [
		"1.61"
	],
	"inherit": "models/vim",
	"vfetcher": {
		"mode": "github",
		"url": "github://hari-rangarajan/CCTree/",
		"regexp": "cctree-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "CCTree-${VERSION}",
	"urls": [
		"github://hari-rangarajan/CCTree/"
	],
	"deps": [
		"dev-util/cscope"
	],
	"steps": {
		"install": [
			"hl_vim_plugin_cp ftplugin/*.vim"
		]
	},
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"cctree"
		],
		"centos7": [
			"cctree"
		],
		"gentoo": [
			"app-vim/cctree"
		]
	}
}
