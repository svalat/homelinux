{
	"name": "app-vim/vimgitlog",
	"homepage": "https://github.com/PAntoine/vimgitlog/",
	"versions": [
		"6.0.0"
	],
	"inherit": "models/vim",
	"vfetcher": {
		"mode": "http",
		"url": "https://github.com/PAntoine/vimgitlog/releases",
		"regexp": "^version_([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*)"
	},
	"md5": {},
	"subdir": "vimgitlog-version_${VERSION}",
	"urls": [
		"https://github.com/PAntoine/vimgitlog/archive/version_${VERSION}.tar.gz"
	],
	"deps": [],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"vimgitlog"
		],
		"centos7": [
			"vimgitlog"
		],
		"gentoo": [
			"app-vim/gitlog"
		]
	}
}
