{
	"name": "app-vim/json",
	"homepage": "https://github.com/elzr/vim-json/",
	"versions": [
		"master"
	],
	"inherit": "models/vim",
	"vfetcher": {
		"mode": "http",
		"url": "",
		"regexp": "json-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "vim-json-${VERSION}",
	"urls": [
		"https://github.com/elzr/vim-json/archive/master.zip"
	],
	"deps": [
		"app-editors/vim"
	],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"json"
		],
		"centos7": [
			"json"
		],
		"gentoo": [
			"app-vim/json"
		]
	}
}
