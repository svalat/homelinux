{
	"name": "app-vim/omnicppcomplete",
	"homepage": "http://www.vim.org/scripts/script.php?script_id=1520",
	"versions": [
		"0.41"
	],
	"inherit": "models/autotools",
	"vfetcher": {
		"mode": "http",
		"url": "http://www.vim.org/scripts/script.php?script_id=1520",
		"regexp": "omnicppcomplete-([0-9]+\\.[0-9]+\\.?[0-9]*\\.?[0-9]*).(tar.bz2|tar.gz|tbz|tar.xz)"
	},
	"md5": {},
	"subdir": "./",
	"urls": [
		"http://www.vim.org/scripts/download_script.php?src_id=7722"
	],
	"steps": {
		"download": [
			"run wget ${URLS} -O omnicppcomplete.zip"
		],
		"extract": [
			"run unzip omnicppcomplete.zip",
			"run rm omnicppcomplete.zip"
		]
	},
	"deps": [
		"app-arch/unzip",
		"dev-util/ctags"
	],
	"configure": {},
	"use": [
		""
	],
	"host": {
		"default": false,
		"debian8": [
			"omnicppcomplete"
		],
		"centos7": [
			"omnicppcomplete"
		],
		"gentoo": [
			"app-vim/omnicppcomplete"
		]
	}
}
